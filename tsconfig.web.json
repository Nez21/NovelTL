{
  "extends": "@electron-toolkit/tsconfig/tsconfig.web.json",
  "include": [
    "src/main/api.ts",
    "src/renderer/src/env.d.ts",
    "src/renderer/src/**/*",
    "src/renderer/src/**/*.tsx",
    "src/renderer/types/**/*",
    "src/preload/*.d.ts",
    "src/types/*.d.ts"
  ],
  "compilerOptions": {
    "composite": true,
    "jsx": "react-jsx",
    "baseUrl": ".",
    "strictNullChecks": true,
    "paths": {
      "@renderer/*": [
        "src/renderer/src/*"
      ]
    }
  }
}
